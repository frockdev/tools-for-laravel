{
    "name": "frock-dev/tools-for-laravel",
    "description": "Toolkit to boost your development with Laravel and frock.dev",
    "autoload": {
        "psr-4": {
            "FrockDev\\ToolsForLaravel\\": "src/"
        }
    },
    "bin": ["bin/frock.php", "bin/frock-server.php", "bin/frock-dev-server.php"],
    "require": {
        "larastan/larastan": "v2.9.0",
        "nette/php-generator": "4.1.3",
        "friendsofphp/php-cs-fixer": "^3.19",
        "spatie/laravel-data": "4.4.1",
        "jonahgeorge/jaeger-client-php": "1.5.0",
        "basis-company/nats": "0.18.0",
        "promphp/prometheus_client_php": "^2.8",
        "barryvdh/laravel-ide-helper": "v3.0.0",
        "swow/swow": "^1.4",
        "laravel/octane": "v2.3.5",
        "spatie/file-system-watcher": "1.2.0",
        "ext-pcntl": "*"
    },
    "require-dev": {
    },
    "extra": {
        "laravel": {
            "providers": [
                "FrockDev\\ToolsForLaravel\\FrockServiceProvider"
            ]
        }
    }
}
