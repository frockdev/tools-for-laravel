{
    "name": "frock-dev/tools-for-laravel",
    "description": "Toolkit to boost your development with Laravel and frock.dev",
    "autoload": {
        "psr-4": {
            "FrockDev\\ToolsForLaravel\\": "src/"
        }
    },
    "require": {
        "nunomaduro/larastan": "v2.6.4",
        "nette/php-generator": "^4.0",
        "friendsofphp/php-cs-fixer": "^3.19",
        "grpc/grpc": "v1.57.0",
        "spatie/laravel-data": "3.9.2",
        "jonahgeorge/jaeger-client-php": "1.5.0",
        "basis-company/nats": "^0.14.0",
        "hyperf/framework": "^3.1",
        "hyperf/grpc-server": "^3.1",
        "hyperf/metric": "^3.1",
        "hyperf/pool": "^3.1",
        "hyperf/process": "^3.1",
        "hyperf/redis": "^3.1"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "v2.13.0",
        "swoole/ide-helper": "~5.0.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "FrockDev\\ToolsForLaravel\\FrockServiceProvider"
            ]
        }
    }
}
